int pushButton =2;

int pushButton2 =3;
 
setup() {
 
  Serial.begin(9600); 
 
  pinMode(pushButton, INPUT);
  pinMode(pushButton2, INPUT);
}
 
void loop() {
 
  int buttonState = digitalRead(pushButton); 
  int buttonState2 = digitalRead(pushButton2); 
 
  Serial.println(buttonState); 
  Serial.println(buttonState2); 
 
  delay(1);
  
}
